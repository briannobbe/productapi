{
    "Name": "The Product API - v1",
    "Description": "Use this API for Listing Available Products to Order",
    "Version": "1",
    "ApiEulaName": "Standard EULA",
    "AccessStatus": "PUBLIC",
    "SsgUrl": "apiproxy/productlocation/v1",
    "PolicyTemplateName": "LAC Service Integration Template Enforcing OAuth 2.0",
    "PolicyTemplateBundle": "LACPortalTemplateBundle.xml",
    "PolicyTemplateArguments": [
        {
            "Name": "sslEnabled",
            "Value": "true"
        },
        {
            "Name": "noAuthSwagger",
            "Value": "true"
        },
        {
            "Name": "LACRole",
            "Value": "Full access"
        },
        {
            "Name": "swaggercacheTTL",
            "Value": "300"
        }
    ],
    "LACBaseURL": "/rest/default/productlocation/v1",
    "SpecFilename": "productswaggerdef.json",
    "LACDBSchema": [{
        "dbPrefix": "main",
        "FileName": "ProductInfoTablesSchema.json"
    }],
    "LACServiceData": [
        {
            "TableName": "ProductDetail",
            "FileName": "ProductDetailData.json"
        },
        {
            "TableName": "ProductLocation",
            "FileName": "ProductLocationData.json"
        }
    ]
}
